interface=wlan1
driver=nl80211
ssid={{ ap_wifi_ssid }}
hw_mode=g
channel=6
macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0
wpa=2
wpa_passphrase={{ vault_ap_wifi_password }}
wpa_key_mgmt=WPA-PSK
rsn_pairwise=CCMP
